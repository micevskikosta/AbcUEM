
@{
    ViewBag.Title = "FirstRegister";
    Layout = null;
}

<form ng-submit="insertFirstUser()">
    <div class="col-lg-6 admin-panel-holder">
        <div class="col-lg-12">
            <h2 class="title-modal">First Register</h2>
        </div>
        <div class="col-lg-6">
            <label class="name-label" for="name">Name</label>
            <input id="name" type="text" ng-model="name" />
        </div>
        <div class="col-lg-6">
                <label class="name-label" for="middleName">Middle Name</label>
                <input id="middleName" type="text" ng-model="middleName" />
            </div>
        <div class="col-lg-6">
            <label class="name-label" for="lastName">Last Name</label>
            <input id="lastName" type="text" ng-model="lastName" />
        </div>
        <div class="col-lg-6">
            <label class="name-label" for="username">Username</label>
                <input id="username" type="text" ng-model="username" />
            </div>
            <div class="col-lg-6">
                    <label class="name-label" for="email">Email</label>
                    <input id="email" type="email" ng-model="email" />
               </div>
        <div class="col-lg-6">
            <label class="name-label" for="password">Password</label>
                <input id="password" type="password" ng-model="password" />
            </div>
        <div class="col-lg-6">
            <label class="name-label" for="confirmPassword">Confirm Password</label>
            <input id="confirmPassword" type="password" ng-model="confirmPassword" />
        </div>
        <div class="col-lg-12">
            <input class="save-first-user" type="submit" value="Save" />
        </div>
        </div>
    <div class="col-lg-6 bg-first-user"></div>
</form>

